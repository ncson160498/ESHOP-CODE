						{{#each data}}
						<div class="col-sm-4">
							<div class="product-image-wrapper">
								<div class="single-products">
									<div class="productinfo text-center">
										<img src="/img/upload/{{image}}" alt="" width="260" height="300"/>
										<h2>{{price}} VND</h2>
										<p>{{name}}</p>
										<a href="/product/detail/{{id}}" class="btn btn-default add-to-cart"><i class="fa fa-search"></i>Xem chi tiết</a>
									</div>
									<div class="product-overlay">
										<div class="overlay-content">
											<h2>{{price}} VND</h2>
											<p>{{name}}</p>
											<a href="/product/detail/{{id}}" class="btn btn-default add-to-cart"><i class="fa fa-search"></i>Xem chi tiết</a>
										</div>
									</div>
								</div>						
							</div>
						</div>
